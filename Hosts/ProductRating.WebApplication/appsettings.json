{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Configurations": {
    "UrlAPI": {
      "AuthRequest": "http://localhost:5227/api/Auth/",
      "ProductRecognitionRequest": "http://localhost:5227/api/ProductRecognition/",
      "ProductRequest": "http://localhost:5227/api/Product/",
      "ReviewRequest": "http://localhost:5227/api/Review/"
    },
    "CookieService": {
      "AuthCookieName": "AuthToken",
      "AuthCookieTime": 30
    },
    "AuthorizationController": {
      "RegistrationView": "~/Views/Auth/Registration.cshtml"
    },
    "RecognitionController": {
      "MainView": "~/Views/Main/Recognition.cshtml"
    }
  }
}