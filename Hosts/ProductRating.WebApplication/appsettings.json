{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Configurations": {
    "CookieService": {
      "AuthCookieName": "AuthToken",
      "AuthCookieTime": 30
    },
    "AuthorizationController": {
      "RegistrationView": "~/Views/Registration/Registration.cshtml"
    },
    "MainController": {
      "RecognitionView": "~/Views/Recognition/Recognition.cshtml",
      "ProductsView": "~/Views/Products/Products.cshtml"
    },
    "RecognitionController": {
      "Url": "https://localhost:7066/api/ProductRecognition",
      "View": "~/Views/Recognition/Recognition.cshtml"
    }
  }
}