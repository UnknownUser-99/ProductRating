{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Configurations": {
    "UrlAPI": {
      "AuthRequest": "https://localhost:7066/api/Auth/",
      "ProductRecognitionRequest": "https://localhost:7066/api/ProductRecognition/",
      "ProductRequest": "https://localhost:7066/api/Product/",
      "ReviewRequest": "https://localhost:7066/api/Review/"
    },
    "CookieService": {
      "AuthCookieName": "AuthToken",
      "AuthCookieTime": 30
    },
    "AuthorizationController": {
      "RegistrationView": "~/Views/Auth/Registration.cshtml"
    },
    "RecognitionController": {
      "MainView": "~/Views/Main/Recognition.cshtml"
    }
  }
}